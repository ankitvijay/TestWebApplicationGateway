<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Redirect rquests to default azure websites domain" stopProcessing="true">
          <match url="(.*)" />
          <conditions logicalGrouping="MatchAny">
            <add input="{HTTP_HOST}" pattern="^test-ag-web1\.azurewebsites\.net$" />
          </conditions>
          <action type="Redirect" url="http://www.ankitvijay.net" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
