<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="TestingRedirect" stopProcessing="true">
          <match url="Applefile.txt" />
          <action type="Rewrite" url="OtherAppleFile.txt" redirectType="Permanent" />
        </rule>
        <rule name="TestingRedirect2" stopProcessing="true">
          <match url="http://ankitvijay.com/SomeAppleFile.txt" />
          <action type="Rewrite" url="OtherAppleFile.txt" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
